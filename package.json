    "pinia": "^2.1.7",
    "qrcode.vue": "^3.4.1",
    "save": "^2.9.0",
    "swiper": "^10.3.1",
    "ts-md5": "^1.3.1",
    "typings": "^0.6.10",
    "vue": "^3.4.21",
    "vue-router": "^4.3.0"
  },
  "devDependencies": {
    "@algolia/client-search": "4.16.0",
    "@types/jquery": "^3.5.29",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
